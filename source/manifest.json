{
  "manifest_version": 2,
  "name": "Brain Redirect",
  "version": "0.1",
  "minimum_chrome_version": "74",

  "description": "Redirects pages from bracklists to challenge sites and redirects back after finishing challenge.",
  "applications": {
  	"gecko": {
  		"id": "addon@example.com",
  		"strict_min_version": "67.0"
  	}
  },

  "icons": {
    "48": "icons/redirect48.png",
    "128": "icons/redirect128.png"
  },
  "permissions": ["webRequest", "<all_urls>", "storage", "alarms", "webNavigation"],
  "options_ui": {
	"chrome_style": true,
	"browser_style": true,
	"page": "options/options.html"
  },
  "background": {
	"page": "brainredirect.html"
  }
 
}


